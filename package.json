{
  "name": "geocola",
  "version": "0.8.0",
  "description": "A configureable openlayers mapping and data management app",
  "repository": "https://github.com/roemhildtg/geocola",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "node build",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "serve": "http-server"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "http-server": "^0.10.0",
    "steal-css": "^1.2.4",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.1.0",
    "steal-tools": "^1.3.6"
  },
  "dependencies": {
    "can-define": "1.5.1",
    "can-event": "3.7.5",
    "can-geo": "^0.3.15",
    "font-awesome": "^4.7.0",
    "openlayers": "^4.1.1",
    "sidebar-v2": "github:turbo87/sidebar-v2",
    "spectre.css": "^0.2.14",
    "steal": "^1.5.2"
  },
  "main": "app/app.js",
  "steal": {
    "plugins": [
      "steal-less",
      "steal-css",
      "steal-stache"
    ],
    "bundleAssets": true,
    "bundle": [
      "geocola/config/default"
    ],
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    }
  }
}
